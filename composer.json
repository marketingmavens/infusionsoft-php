{
    "name": "infusionsoft/php-sdk",
    "description": "PHP SDK for the Infusionsoft",
    "keywords": [
        "infusionsoft",
        "sdk"
    ],
    "homepage": "https://developer.infusionsoft.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Infusionsoft",
            "homepage": "https://developer.infusionsoft.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": "^7.0.1",
        "lstrojny/fxmlrpc": "^0.15.0",
        "php-http/guzzle7-adapter": "^0.1.0",
        "php-http/message": "^1.9",
        "laminas/laminas-diactoros": "^2.4",
        "illuminate/collections": "^8.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.3",
        "doctrine/instantiator": "1.3.1",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-0": {
            "Infusionsoft": [
                "src",
                "tests"
            ]
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Infusionsoft\\FrameworkSupport\\Laravel\\InfusionsoftServiceProvider"
            ]
        }
    }
}
